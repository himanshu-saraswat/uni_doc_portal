{
    "plugins": ["@babel/plugin-proposal-optional-chaining"],
    "presets": [
      [
        "babel-preset-gatsby",
        {
          "targets": {
            "browsers": [">0.25%", "not dead"]
          }
        }
      ],
      
      [
        "@babel/preset-env",
        {
          "loose": true,
          "modules": false,
          "useBuiltIns": "usage",
          "shippedProposals": true,
          "targets": {
            "browsers": [">0.25%", "not dead"],
          }
        }
      ],
      [
        "@babel/preset-react",
        {
          "useBuiltIns": true,
          "pragma": "React.createElement",
        }
      ],
      ["@babel/preset-flow"]
    ]
  }